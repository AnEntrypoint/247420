{
  "name": "247420-community-app",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:ssr": "NODE_ENV=development node server.js",
    "build": "vite build",
    "build:ssr": "node build-ssr.js",
    "build:vite-only": "vite build",
    "build:production": "npm run build:ssr",
    "build:media": "node scripts/build-media-lists.js",
    "build:optimize": "node scripts/optimize-build.js",
    "build:full": "npm run build && npm run build:media && npm run build:optimize",
    "test:scheduler": "node scripts/test-scheduler.js",
    "preview": "vite preview",
    "preview:ssr": "NODE_ENV=production node server.js",
    "start": "NODE_ENV=production node server.js",
    "start:dev": "NODE_ENV=development node server.js",
    "deploy:ssr": "npm run build:ssr && git add . && git commit -m 'build: SSR deployment build' && git push",
    "deploy": "npm run build && git add . && git commit -m 'build: deployment build' && git push",
    "deploy:optimized": "npm run build:full && git add . && git commit -m 'build: optimized deployment' && git push"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "420kit-shared": "file:../420kit/420kit-shared-1.0.1.tgz",
    "express": "^4.21.2"
  }
}
